---
import UmaLayout from "../../layouts/UmaLayout.astro";

const title = "rosy dream";
const cardname = "rosy dream";
const date = "06-08-2025";
const rank = "★★★★";
const racetype = "turf, med, long";
const strat = "pace chaser";
const potential = "3";
const cardimg =
    "https://gametora.com/images/umamusume/characters/chara_stand_1030_103001.png";
---

<UmaLayout
    title={title}
    ,
    cardname={cardname}
    ,
    date={date}
    ,
    rank={rank}
    ,
    racetype={racetype}
    ,
    strat={strat}
    ,
    potential={potential}
    ,
    cardimg={cardimg}
>
    <div slot="box-left">
        <table>
            <tr>
                <th>trait</th>
                <th>base</th>
                <th>bonus</th>
            </tr>
            <tr>
                <td>Speed</td>
                <td>79</td>
                <td></td>
            </tr>
            <tr>
                <td>Stam</td>
                <td>130</td>
                <td>10%</td>
            </tr>
            <tr>
